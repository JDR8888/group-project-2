    <nav class="navbar" style="background-color: #C68CD9;">
      <nav class="navbar navbar-expand-lg bg-body-tertiary">
        <div class="container-fluid">
          <img
            src="/images/Love-hungry-1.png"
            alt="logo"
            height="100"
            width="100"
          />
          <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
            <div class="navbar-nav">
              <a class="nav-link" aria-current="page" href="/dating">Dating</a>
              <a class="nav-link" aria-current="page" href="#">History</a>
              {{#if logged_in}}
                <button
                  id="logout"
                  class="nav-link btn"
                  aria-current="page"
                >Logout</button>
              {{else}}
                <a class="nav-link" aria-current="page" href="/login">Login</a>
              {{/if}}
            </div>
          </div>
        </div>
      </nav>
    </nav>

    {{{body}}}

