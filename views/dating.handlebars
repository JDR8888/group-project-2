body {
 background-image: url("paper.gif");
 background-color: #cccccc;
}
<div class="card-title d-flex justify-content-center card-body text-danger">
  <h2 class=" ">Dating Portal</h2>
</div>
<div class="card-text d-flex justify-content-left card-body text-danger">
  <h3>Other users in {{location}} that want {{what_to_eat}}</h3>
</div>
  <section id="dating results">
    {{> user-details}}
  </section>

  <section>
    <div class="card-text d-flex justify-content-left card-body text-danger">
    <h3>Suggested {{what_to_eat}} Restaraunts:</h3>
    </div>
    {{#each displayRestaurants as |restaraunt|}}
    <div class="card mb-3">
      <div class="row no-gutters">
        <div class="col-md-4">
          <img src="{{restaraunt.icon}}" class="card-img" alt="picture of food" />
        </div>
        <div class="col-md-8">
          <div class="card-body">
            <h5 class="card-title">{{restaraunt.name}}</h5>
            <p class="card-text"><strong>{{restaraunt.dba}}</strong></p>

          </div>
        </div>
      </div>
    </div>
    {{/each}}
  </section>
  <div class="text-center ">
    <a href=" #" class="btn btn-danger ">Go somewhere</a>
  </div>

